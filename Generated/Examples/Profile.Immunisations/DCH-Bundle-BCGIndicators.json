              "title": "Indicator BCG",
              "question": [
                {
                  "text": "An indication as to whether BCG vaccine is required",
                  "answer": [
                    {
                      "valueBoolean": true
                    },
                    {
                      "valueReference": {
                        "reference": "urn:uuid:1d67c470-4f45-11e7-b114-b2f933d5fe66"
                      }
                    }
                  ]
                },
                {
                  "text": "The reason why BCG vaccine is indicated",
                  "answer": [
                    {
                      "valueString": "Parent/grandparent born in a country with a high TB rate"
                    },
                    {
                      "valueReference": {
                        "reference": "urn:uuid:1d67c470-4f45-11e7-b114-b2f933d5fe66"
                      }
                    }
                  ]
                },
                {
                  "text": "The country of birth of parent or grandparent who was born in a country with a high TB rate",
                  "answer": [
                    {
                      "valueString": "South Africa"
                    },
                    {
                      "valueReference": {
                        "reference": "urn:uuid:1d67c470-4f45-11e7-b114-b2f933d5fe66"
                      }
                    }
                  ]
                }
              ]
            }
          ]
